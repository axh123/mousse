FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.5;

deltaT          0.005;

writeControl    timeStep;

writeInterval   20;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    fileUpdate1
    {
        type            timeActivatedFileUpdate;
        functionObjectLibs ("libmousse_utility_function_objects.so");
        outputControl   timeStep;
        outputInterval  1;
        fileToUpdate    "$MOUSSE_CASE/system/fvSolution";

        timeVsFile
        (
            ( -1   "$MOUSSE_CASE/system/fvSolution.0"  )
            ( 0.10 "$MOUSSE_CASE/system/fvSolution.10" )
            ( 0.20 "$MOUSSE_CASE/system/fvSolution.20" )
            ( 0.35 "$MOUSSE_CASE/system/fvSolution.35" )
        );
    }
}

// vim: set ft=openfoam et sw=4 ts=4 sts=4:
