// mousse: CFD toolbox

FoamFile
{
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      k;
}

dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 3.75e-3;

boundaryField
{
  rotor
  {
    type            kqRWallFunction;
    value           uniform 3.75e-3;
  }
  freestream
  {
    type            inletOutlet;
    inletValue      uniform 3.75e-3;
  }
  front
  {
    type            empty;
  }
  back
  {
    type            empty;
  }
}

