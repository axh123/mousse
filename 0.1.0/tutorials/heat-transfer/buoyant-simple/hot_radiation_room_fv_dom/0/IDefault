// mousse: CFD toolbox

FoamFile
{
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      IDefault;
}

dimensions      [1 0 -3 0 0 0 0];
internalField   uniform 0;

boundaryField
{
  ".*"
  {
    type            greyDiffusiveRadiation;
    T               T;
    emissivityMode  lookup;
    emissivity      uniform 1.0;
    value           uniform 0;
  }
}

