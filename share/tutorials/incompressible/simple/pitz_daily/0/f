// mousse: CFD toolbox

FoamFile
{
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      v2;
}

dimensions      [0 0 -1 0 0 0 0];
internalField   uniform 0;

boundaryField
{
  inlet
  {
    type            zeroGradient;
  }
  outlet
  {
    type            zeroGradient;
  }
  upperWall
  {
    type            fWallFunction;
    value           uniform 0;
  }
  lowerWall
  {
    type            fWallFunction;
    value           uniform 0;
  }
  frontAndBack
  {
    type            empty;
  }
}

