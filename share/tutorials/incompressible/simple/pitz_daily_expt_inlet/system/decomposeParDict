// mousse: CFD toolbox

FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "system";
  object      decomposeParDict;
}

numberOfSubdomains 4;
method          hierarchical;
simpleCoeffs
{
  n               (2 1 1);
  delta           0.001;
}
hierarchicalCoeffs
{
  n               (2 2 1);
  delta           0.001;
  order           xyz;
}
manualCoeffs
{
  dataFile        "";
}
distributed     no;
roots           ( );

